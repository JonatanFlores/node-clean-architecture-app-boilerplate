{
  "name": "node-clean-architecture-app-boilerplate",
  "version": 2,
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "./dist/main/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/dist/main/index.js"
    }
  ]
}
