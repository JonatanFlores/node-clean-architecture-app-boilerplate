{
  "name": "express-boilerplate",
  "version": "0.0.1",
  "description": "Express and Jest boilerplate",
  "main": "./dist/main/index.js",
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "node",
    "typescript",
    "jest"
  ],
  "author": {
    "name": "Jonatan Flores",
    "email": "jonatafloress@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/main",
    "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "prepare": "husky install",
    "check": "npm-check -s -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "test:integration": "npm t -- --config ./jest.integration.config.js",
    "test:coveralls": "npm run test:coverage && coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "@shelf/jest-mongodb": "^2.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.4.2",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "coveralls": "^3.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^11.1.2",
    "mockdate": "^3.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^4.2.2",
    "rimraf": "^3.0.2",
    "validator": "^13.6.0"
  }
}
